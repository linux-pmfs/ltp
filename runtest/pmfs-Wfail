#ADS2006 aio-stress -I500  -o2 -O -r32  -t2  $TMPDIR/junkfile /test/aiodio2/file2
#ADS2007 aio-stress -I500  -o3 -O -r8   -t2  $TMPDIR/junkfile /test/aiodio2/file2
#ADS2008 aio-stress -I500  -o3 -O -r16  -t2  $TMPDIR/junkfile /test/aiodio2/file2
#ADS2009 aio-stress -I500  -o3 -O -r32  -t4  $TMPDIR/junkfile /test/aiodio2/file2 $TMPDIR/file3 /test/aiodio2/file4
